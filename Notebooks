        "print(f\"Average length before: {original_avg:.1f} characters\")\n",
        "print(f\"Average length after: {normalized_avg:.1f} characters\")\n",
        "print(f\"Length reduction: {((original_avg - normalized_avg) / original_avg) * 100:.1f}%\")"
